#include <iostream>

using namespace std;

int main()
{
    int NC;
    scanf("%d", &NC);
    int q=NC;
    while (NC--){
        int a, b, c, d;
        scanf("%d %d %d", &a, &b, &c);
        if(a<b&&b<c){d=b;};
        if(c<b&&b<a){d=b;};
        
        if(a<c&&c<b){d=c;};
        if(b<c&&c<a){d=c;};
        
        if(b<a&&a<c){d=a;};
        if(c<a&&a<b){d=a;};
        
        printf("Case %d: %d\n", q-NC, d);
        
        
    }
   
   
   
   
   
}
