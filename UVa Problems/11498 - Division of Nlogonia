
#include <iostream>
#include <string>

int main(){
    int NC; //number of cases
    int n, m; //orgin
    int x, y; //cordinate
    
    while( scanf("%d", &NC), NC){// while not 0;
        scanf("%d %d", &n, &m); //set orgin
        
        while (NC--){
            scanf("%d %d", &x, &y); 
            if (x==n || y==m){ printf ("%s\n", "divisa");}
            else if(x<n && y>m){printf ("%s\n", "NO");}
            else if(x>n && y>m){printf ("%s\n", "NE");}
            else if(x>n && y<m){printf ("%s\n", "SE");}
            else if(x<n && y<m){printf ("%s\n", "SO");}
            
        }
    }
}
